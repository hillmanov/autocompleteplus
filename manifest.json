      "css": ["css/redmond/jquery-ui-1.8.20.custom.css", "css/override.css", "css/ui.achtung-min.css"],
      "js": ["lib/jquery-1.7.2.min.js", "lib/jquery-ui-1.8.20.custom.min.js", "lib/ui.achtung-min.js", "main.js"]
    }
  ],
  "permissions": [
    "contextMenus", 
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}